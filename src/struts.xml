<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="user-action" extends="struts-default">
	<!-- 拦截器 -->
	<interceptors>
		<interceptor name="abc" class="interceptor.AbcInterceptor"></interceptor>
		<interceptor-stack name="mystack">
			<interceptor-ref name="defaultStack"></interceptor-ref>
			<interceptor-ref name="abc"></interceptor-ref>
		</interceptor-stack>
	</interceptors>
	<!-- 用户登录 -->
	<action name="loginAction" class="" method="execute">
		<result name="success">/index.html</result>
		<result name="input">/login.html</result>
	</action>
	<action name="selectAction" class="" method="execute">
		<result>/shop-list.html</result>
	</action>
	</package>
</struts>